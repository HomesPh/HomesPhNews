# --- STORAGE CONFIGURATION ---
STORAGE_PROVIDER=gcp

# Google Cloud Storage (GCP)


GCP_PROJECT_ID=your-project-id
GCP_BUCKET_NAME=your-gcs-bucket-name
# Paste the entire content of your service-account.json here as a single-line string
GCP_SERVICE_ACCOUNT_JSON={"type": "service_account", "project_id": "...", ...}

# Amazon S3

AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=ap-southeast-1
S3_BUCKET_NAME=your-s3-bucket-name

# --- DATABASE / CACHE ---

REDIS_URL=redis://localhost:6379/0


# --- AI SERVICES (Nano Banana / Google) ---
# API Key for Nano Banana (Image Generation)
NANO_BANANA_API_KEY=your-nano-banana-key

# Google AI (Gemini/Vertex) for Summarization and Paraphrasing
GOOGLE_AI_API_KEY=your-google-ai-key

# --- SCRAPER SETTINGS ---
# Languages/Search keywords
SCRAPER_KEYWORDS=Philippines real estate, Homes PH, Manila property news
